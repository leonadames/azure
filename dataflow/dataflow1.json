					"dataset": {
						"referenceName": "Dataset_Order",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          orderIdentifier as string,",
				"          orderType as string,",
				"          {vendor.organizationIdentifier} as string,",
